<!--pages/shopList/shopList.wxml-->
<!-- <text>pages/shopList/shopList.wxml</text> -->
<view class='shop'>
  <image class='shop-image' src='/assets/images/shinnnosuke.png'></image>
  <view class='shop-info'>
    <text class='shop-title'>店铺地址</text>
    <text class='shop-tel'>电话</text>
    <text class='shop-address'>地址</text>
    <text class='shop-time'>营业时间</text>
  </view>
  <view class='shop-star'>
    <view class='star-after'>★★★★★</view>
    <view class='star-before'>★★★★★</view>
  </view>
</view>